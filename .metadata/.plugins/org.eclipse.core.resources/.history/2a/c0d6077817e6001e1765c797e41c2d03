package StartP;
import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		boolean loop = false;
		while(loop == false) {
			Scanner scanner = new Scanner (System.in);
			int escolha = scanner.nextInt();
			switch(escolha) {
			case 1:
				System.out.print(1);
				break;
				
			case 2:
				System.out.print(2);
				break;
			}
		}

	}

}
	